<h1>Listing pictures</h1>

<table>
  <tr>
    <th>Pic</th>
    <th>Name</th>
    <th>Dive site</th>
    <th>Pict date</th>
    <th>Tags</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @pictures.each do |picture| %>
  <tr>
    <td><%= picture.pic_id %></td>
    <td><%= picture.name %></td>
    <td><%= picture.dive_site %></td>
    <td><%= picture.pict_date %></td>
    <td><%= picture.tags %></td>
    <td><%= link_to 'Show', picture %></td>
    <td><%= link_to 'Edit', edit_picture_path(picture) %></td>
    <td><%= link_to 'Destroy', picture, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Picture', new_picture_path %>
