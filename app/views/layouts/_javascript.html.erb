<script type="text/javascript">
$(function(){
  var $container = $('.slideshow').cycle({
    speed: 300,
    autostop: 1,
    autostopCount: 1
  });

  var obj= $('#thumbnail img');
  $container.children().each(function(i){
    $(obj[i]).css('cursor', 'pointer').click(function(){
        $container.cycle(i);
        return false;
    });
  });
});
</script>
